      "children": [
        {
          "bold": true,
          "text": "6. Required Materials"
        }
      ]
    },
    {
      "id": "33c91e5d-8a0c-4c56-ade4-533853a88f0e",
      "type": "h2",
      "children": [
        {
          "bold": true,
          "text": ""
        }
      ]
