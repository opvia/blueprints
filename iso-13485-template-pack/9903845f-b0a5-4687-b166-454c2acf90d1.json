      "indent": 0,
      "children": [
        {
          "id": "ed73f3f3-387a-478f-91c6-7b6427817155",
          "type": "lic",
          "children": [
            {
              "bold": true,
              "text": "Fire Safety Training"
            },
            {
              "text": ": All lab personnel must receive fire safety training, including how to use fire extinguishers and respond to emergencies. Repeat annually."
            }
          ]
        }
      ]
    },
