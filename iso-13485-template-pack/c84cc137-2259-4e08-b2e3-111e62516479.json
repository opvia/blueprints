    {
      "id": "000538c2-8355-47b3-a4f2-29b43731c94f",
      "type": "li",
      "indent": 0,
      "children": [
        {
          "id": "00991a3e-91b7-4137-8ea2-fe56b5a078b1",
          "type": "lic",
          "children": [
            {
              "bold": true,
              "text": "Overview"
            },
            {
              "text": ": Ensures compliance with quality standards and manages audits."
            }
          ]
        }
      ]
    },
    {
      "id": "037f5ca7-8bbc-4c13-88db-5e5d970a320a",
